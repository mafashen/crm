<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping package="entity.job">
	<class name="Visit" table="t_visit">
		<id name="visit_id">
			<generator class="native" />			
		</id>
		<property name="visit_time"/>
		<property name="visit_content"/>
		
		<many-to-one name="student" class="entity.student.Student" lazy="false"></many-to-one>
		<property name="visit_img"/>

		
	</class>
</hibernate-mapping>